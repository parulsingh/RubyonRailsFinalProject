<h1>General CS Jokes</h1>
<% @joke.each do |joke| %>

  <% if joke.jokeclass == 'General' %>
  	<h3> <%= joke.content %></h3>
  	<%= commontator_thread(joke) %>
  	  <%=link_to "upvote",{:controller => "jokes", :action => "upvote", :id => joke.id }, method: :put %>
  <%= joke.score %>
  <% end %>
<% end %>